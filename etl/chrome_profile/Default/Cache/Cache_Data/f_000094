    (function() {
      function __getHngCookie() {
        var name = 'hng=';
        var res = document.cookie.split(';');
        for (let i = 0; i < res.length; i++) {
          var data = res[i].trim();
          if (data.indexOf(name) == 0) {
            return data.substring(name.length, data.length);
          }
        }
        var defaultHngMap = {
          'sg': 'SG|en-US|SGD|702',
          'vn': 'VN|vi|VND|704',
          'ph': 'PH|en-PH|PHP|608',
          'my': 'MY|en-US|MYR|458',
          'th': 'TH|en|THB|764',
          'id': 'ID|id|IDR|360',
        }
        var venture = location.hostname.split('.').pop();
        return defaultHngMap[venture];
      }
  
      function __setCookie(name, value, maxAge) {
        var domain = document.domain.substring(document.domain.indexOf('.'));
        var newCookie = name + '=' + value + ';domain=' + domain + ';path=/';
        if (maxAge) {
          newCookie = newCookie + ';max-age=' + maxAge;
        }
        
        document.cookie = newCookie;
      }
  
      if (__IS_IN_MAPP__) {
        var url = new URL(location.href);
        var wh_language = url.searchParams.get('wh_language');
        if (wh_language) {
          var arr = __getHngCookie().split('|');
          if (arr.length > 1) {
            arr[1] = wh_language;
          }
          __setCookie('hng', arr.join('|'), 2592000);
          console.log('sync cookie success');
        }

        var mapp_sid = url.searchParams.get('mapp_sid');
        if (mapp_sid) {
          __setCookie('lzd_sid', mapp_sid);
        }
      }
    })()
