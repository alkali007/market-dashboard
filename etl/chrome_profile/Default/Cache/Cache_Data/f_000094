    var __IS_IN_MAPP__ = navigator.userAgent.indexOf('MiniProgram') > -1 || navigator.userAgent.indexOf('AlipayIDE') > -1;

    if (__IS_IN_MAPP__) {
      document.writeln('<script src="https://appx/web-view.min.js"' + '>' + '<' + '/' + 'script>');
      document.writeln('<script src="https://g.lazcdn.com/g/code/npm/@ali/lib-mapp-bridge/1.0.1/index.umd.es5.production.js"' + '>' + '<' + '/' + 'script>');
