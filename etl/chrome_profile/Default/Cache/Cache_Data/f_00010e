      });
       window.addEventListener('unhandledrejection', (event) => {
        event.preventDefault();
        console.log('unhandledrejection fired: ', event);
        console.log('unhandledrejection reason: ', event.reason);
        window.__wpk &&
          window.__wpk.report &&
          window.__wpk.report({
            bid: 'irmajibm-qikbi6z1',
            category: 110,
